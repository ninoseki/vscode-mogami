{
  "name": "vscode-mogami",
  "displayName": "Mogami",
  "description": "A VS Code extension for checking the latest version of each dependency",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-mogami.git"
  },
  "version": "0.0.2",
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "pyproject",
    "python",
    "gem",
    "ruby"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pyproject.toml",
    "workspaceContains:requirements.txt",
    "workspaceContains:*-requirements.txt",
    "workspaceContains:*.requirements.txt",
    "workspaceContains:*.gemspec",
    "workspaceContains:Gemfile.lock",
    "workspaceContains:Gemfile"
  ],
  "main": "./dist/extension.js",
  "icon": "./images/icon.png",
  "files": [
    "dist"
  ],
  "contributes": {
    "commands": []
  },
  "scripts": {
    "build": "tsup",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "lint": "eslint src test --ext ts",
    "pack": "vsce package",
    "prepare": "husky install",
    "publish": "vsce publish",
    "test": "jest",
    "vscode:prepublish": "npm run build -- --minify",
    "watch": "npm run build -- --sourcemap"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint -- --fix"
    ]
  },
  "devDependencies": {
    "@types/iarna__toml": "^2.0.2",
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "semver": "^7.5.4"
  }
}
