{
  "name": "vscode-mogami",
  "displayName": "Mogami",
  "description": "A VS Code extension for checking the latest version of each dependency",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-mogami.git"
  },
  "version": "0.0.4",
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "pyproject",
    "python",
    "gem",
    "ruby"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pyproject.toml",
    "workspaceContains:requirements.txt",
    "workspaceContains:requirements-*.txt",
    "workspaceContains:*-requirements.txt",
    "workspaceContains:*.requirements.txt",
    "workspaceContains:*.gemspec",
    "workspaceContains:Gemfile.lock",
    "workspaceContains:Gemfile"
  ],
  "main": "./dist/extension.js",
  "icon": "./images/icon.png",
  "files": [
    "dist"
  ],
  "contributes": {
    "commands": []
  },
  "scripts": {
    "build": "tsup",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "lint": "eslint src test --ext ts --fix",
    "format": "prettier --write src test",
    "pack": "vsce package",
    "prepare": "husky",
    "publish": "vsce publish",
    "test": "jest",
    "vscode:prepublish": "npm run build -- --minify",
    "watch": "npm run build -- --sourcemap"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "devDependencies": {
    "@types/iarna__toml": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.20",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "axios": "^1.6.7",
    "axios-cache-interceptor": "^1.5.1",
    "neverthrow": "^6.1.0",
    "semver": "^7.6.0",
    "zod": "^3.22.4"
  }
}
